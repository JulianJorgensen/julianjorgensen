{
  "presets": [
    "next/babel"
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "components": "./components",
          "content": "./content",
          "marquees": "./marquees",
          "assets": "./assets",
          "fonts": "./assets/fonts",
          "store": "./store",
          "utils": "./utils"
        }
      }
    ],
    [
      "styled-components",
      { 
        "ssr": true, 
        "displayName": true,
        "preprocess": false
      }
    ],
    "transform-decorators-legacy",
    "transform-class-properties",
    "inline-react-svg"
  ]
}
